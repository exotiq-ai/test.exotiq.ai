{
  "name": "exotiq-landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pre-deploy": "node scripts/pre-deployment-check.js",
    "verify-build": "node scripts/verify-build.js",
    "health-check": "node scripts/health-check.js",
    "deploy-safe": "npm run pre-deploy && npm run build && npm run verify-build && npm run health-check && echo '✅ Ready for deployment!'",
    "deploy-clean": "rm -rf node_modules package-lock.json dist && npm install && npm run deploy-safe",
    "deploy-reliable": "npm run pre-deploy && npm run build && npm run verify-build && npm run health-check && echo '✅ Reliable deployment ready!'",
    "optimize-images": "node scripts/optimize-images.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "@types/uuid": "^10.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.21.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.33.5",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
